<% include ../partials/header %>
<h2><%= title %></h2>
<form method="POST" action="<%= (projeto._id)?"?_method=PUT":"" %>">
	<%= (typeof(success) != "undefined")?success:'' %>
	<div class="form-group">
		<label>Nome do Projeto</label><input name='projeto[nome]' class="form-control" id='projeto_nome' type='text' required value='<%= projeto.nome %>' />
	</div>
	<div class="form-group">
		<label>Data de In√≠cio</label><input name='projeto[data_inicio]' class="form-control" id='projeto_data_inicio' type='date' required value='<%= moment(projeto.data_inicio).format('YYYY-MM-DD') %>' />
	</div>
	<input type='submit' class="btn btn-success" />
</form>
<% include ../partials/footer %>